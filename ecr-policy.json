{
  "Version": "2008-10-17",
  "Statement": [
    {
      "Sid": "AllowStrapiUserPush",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::265766434095:user/Strapi"
      },
      "Action": [
        "ecr:InitiateLayerUpload",
        "ecr:UploadLayerPart",
        "ecr:CompleteLayerUpload",
        "ecr:PutImage",
        "ecr:BatchCheckLayerAvailability",
        "ecr:GetDownloadUrlForLayer",
        "ecr:BatchGetImage"
      ]
    }
  ]
}
